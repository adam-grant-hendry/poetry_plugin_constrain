{
    "allowCompoundWords": true,
    "dictionaries": [
        "acronyms",
        "authors",
        "english_contractions_and_possessives",
        "fonts",
        "general_ignores",
        "general_terms",
        "placeholders",
        "programs",
        "pytest_fixtures",
        "python-common",
        "python-packages",
        "python",
        "softwareTerms",
        "wordsEn"
    ],
    "dictionaryDefinitions": [
        {
            "addWords": true,
            "description": "Acronyms used in this project.",
            "name": "acronyms",
            "path": "./.cspell/acronyms.txt",
            "scope": "workspace"
        },
        {
            "addWords": true,
            "description": "Names of python packages used in this project.",
            "name": "python-packages",
            "path": "./.cspell/python-packages.txt",
            "scope": "workspace"
        },
        {
            "addWords": true,
            "description": "Names of authors contributing to this project.",
            "name": "authors",
            "path": "./.cspell/authors.txt",
            "scope": "workspace"
        },
        {
            "addWords": true,
            "description": "Names used as placeholders/dummies throughout this package.",
            "name": "placeholders",
            "path": "./.cspell/placeholders.txt",
            "scope": "workspace"
        },
        {
            "addWords": true,
            "description": "General terms used throughout this package.",
            "name": "general_terms",
            "path": "./.cspell/general_terms.txt",
            "scope": "workspace"
        },
        {
            "addWords": true,
            "description": "English contractions and possessives.",
            "name": "english_contractions_and_possessives",
            "path": "./.cspell/english_contractions_and_possessives.txt",
            "scope": "workspace"
        },
        {
            "addWords": true,
            "description": "Names of pytest fixtures.",
            "name": "pytest_fixtures",
            "path": "./.cspell/pytest_fixtures.txt",
            "scope": "workspace"
        },
        {
            "addWords": true,
            "description": "Font names.",
            "name": "fonts",
            "path": "./.cspell/fonts.txt",
            "scope": "workspace"
        },
        {
            "addWords": true,
            "description": "List of prefix/suffix items that can be safely ignored.",
            "name": "general_ignores",
            "path": "./.cspell/general_ignores.txt",
            "scope": "workspace"
        },
        {
            "addWords": true,
            "description": "Names of programs.",
            "name": "programs",
            "path": "./.cspell/programs.txt",
            "scope": "workspace"
        }
    ],
    "flagWords": [
        "hte"
    ],
    "ignorePaths": [
        "*.dll",
        "*.html",
        "*.css",
        "*.json",
        "*.git/**",
        "**/build/**",
        "**/_build/**",
        "**/dist/**",
        ".venv/",
        "*.bat",
        "**/*.pyi",
        ".vscode/"
    ],
    "ignoreRegExpList": [
        "Email",
        "/\\b(don't|didn't|doesn't|isn't|wasn't|wouldn't|won't)\\b/gi"
    ],
    "includeRegExpList": [
        "comments",
        "docstring",
        "string"
    ],
    "useGitignore": true,
    "version": "0.2"
}
